lamd:md_DTN a skos:Concept ;
    dct:created "2021-08-04"^^xsd:date ;
    dct:type "data property" ;
    skos:definition "A sequential number representing the original reference number of the act. In some instances composed or non-standardised numbers are attributed (e.g. treaties)."@en ;
